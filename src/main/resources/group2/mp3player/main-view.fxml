<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="group2.mp3player.MainController" prefHeight="228.0" prefWidth="266.0">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>

  <center>
    <Label fx:id="welcomeText" BorderPane.alignment="CENTER" />
  </center>

  <bottom>
    <Button onAction="#onPlayButtonClick" text="Play MP3" BorderPane.alignment="CENTER" />
  </bottom>
</BorderPane>